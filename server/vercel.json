{
  "version": 2,
  "builds": [
    {
      "src": "./functions.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/signin",
      "dest": "/functions.js"
    },
    {
      "src": "/register",
      "dest": "/functions.js"
    },
    {
      "src": "/session-post",
      "dest": "/functions.js"
    },
    {
      "src": "/session-load",
      "dest": "/functions.js"
    },
    {
      "src": "/session-update",
      "dest": "/functions.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
